{
"compilerOptions": {
    "target": "es5",
    "typeRoots": [
    "./typings",
    "./node_modules/@types"
    ],
    "lib": [
    "dom",
    "dom.iterable",
    "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
},
"include": [
    "**/*.ts",
    "**/*.tsx"
],
"exclude": [
    "node_modules"
]
}